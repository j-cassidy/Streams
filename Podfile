platform :ios, '11.0'

source 'https://github.com/CocoaPods/Specs.git'
target ‘Streams’ do
  use_frameworks!
    pod 'Firebase/Core'
    pod 'Firebase/Storage'
    pod 'Firebase/Firestore'
    pod 'FirebaseUI/Auth'
    pod 'FirebaseUI/Google'
    pod 'Alamofire', '~> 4.7’
    pod "HTMLReader"
    pod 'Kanna', '~> 4.0.0'
    pod 'Stripe'
end
